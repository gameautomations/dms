root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
indent_size = 4
tab_width = 4
max_line_length = 120
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false


[*.{html,css,less,scss,js,jsx,ts,tsx,vue,md,mdx,json,xml,yaml,yml,toml,csproj,vcxproj}]
indent_size = 2
tab_width = 2
